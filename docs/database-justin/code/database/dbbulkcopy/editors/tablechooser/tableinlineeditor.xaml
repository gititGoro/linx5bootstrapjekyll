<UserControl x:Class="Twenty57.Linx.Components.Database.DbBulkCopy.Editors.TableChooser.TableInLineEditor"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
			 xmlns:pe="clr-namespace:System.Activities.Presentation.PropertyEditing;assembly=System.Activities.Presentation"
			 xmlns:pluginUI="clr-namespace:Twenty57.Linx.Plugin.UI.Editors;assembly=Twenty57.Linx.Plugin.UI"
             mc:Ignorable="d" 
             d:DesignHeight="20" d:DesignWidth="300">
	<DockPanel x:Name="mainPanel" LastChildFill="True">
		<pe:EditModeSwitchButton TargetEditMode="Dialog" Name="EditButton" DockPanel.Dock="Right" Command="pluginUI:PropertyEditor.EditValue" CommandParameter="{Binding}">...</pe:EditModeSwitchButton>
	</DockPanel>
</UserControl>
